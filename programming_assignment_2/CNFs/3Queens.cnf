# # Must be Queen in Every Column
# (or Q11 Q12 Q13)
Q11 Q12 Q13

# (or Q21 Q22 Q23)
Q21 Q22 Q23

# (or Q31 Q32 Q33)
Q31 Q32 Q33

# 
# # Must be Queen in Every Row
# (or Q11 Q21 Q31)
Q11 Q21 Q31

# (or Q12 Q22 Q32)
Q12 Q22 Q32

# (or Q13 Q23 Q33)
Q13 Q23 Q33

# 
# # Not Multiple Queens in Column 1
# (not (and Q11 (or Q12 Q13)))
-Q12 -Q11
-Q13 -Q11

# (not (and Q12 (or Q13)))
-Q13 -Q12

# 
# # Not Multiple Queens in Column 2
# (not (and Q21 (or Q22 Q23)))
-Q22 -Q21
-Q23 -Q21

# (not (and Q22 (or Q23)))
-Q23 -Q22

# 
# # Not Multiple Queens in Column 3
# (not (and Q31 (or Q32 Q33)))
-Q32 -Q31
-Q33 -Q31

# (not (and Q32 (or Q33)))
-Q33 -Q32

# 
# # Not Multiple Queens in Row 1
# (not (and Q11 (or Q21 Q31)))
-Q21 -Q11
-Q31 -Q11

# (not (and Q21 (or Q31)))
-Q31 -Q21

# 
# # Not Multiple Queens in Row 2
# (not (and Q12 (or Q22 Q32)))
-Q22 -Q12
-Q32 -Q12

# (not (and Q22 (or Q32)))
-Q32 -Q22

# 
# # Not Multiple Queens in Row 3
# (not (and Q13 (or Q23 Q33)))
-Q23 -Q13
-Q33 -Q13

# (not (and Q23 (or Q33)))
-Q33 -Q23

# 
# # Not Multiple Queens in Same Right Diagonal
# (not (and Q11 (or Q22 Q33)))
-Q22 -Q11
-Q33 -Q11

# (not (and Q12 (or Q23)))
-Q23 -Q12

# (not (and Q21 (or Q32)))
-Q32 -Q21

# (not (and Q22 (or Q33)))
-Q33 -Q22

# 
# # Not Multiple Queens in Same Left Diagonal
# (not (and Q12 (or Q21)))
-Q21 -Q12

# (not (and Q22 (or Q31)))
-Q31 -Q22

# (not (and Q13 (or Q22 Q31)))
-Q22 -Q13
-Q31 -Q13

# (not (and Q23 (or Q32)))
-Q32 -Q23

